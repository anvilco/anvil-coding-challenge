{
  "name": "anvil-react-full-stack-coding-challenge",
  "version": "1.0.0",
  "description": "Anvil's interview challenge base webapp",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "db:reset": "rm src/db/dev.json",
    "lint": "yarn eslint 'src/**/*.js' 'test/**/*.js'",
    "test:server": "NODE_ENV=test mocha 'test/server/**/*.test.js' --require test/server/environment.js",
    "test:client": "NODE_ENV=test mocha 'test/client/**/*.test.js' --require test/client/environment.js --require @babel/register",
    "test": "yarn lint && yarn test:server && yarn test:client"
  },
  "author": "Anvil Foundry Inc.",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "app-module-path": "^2.2.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "lodash-id": "^0.14.1",
    "lowdb": "^1",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.0.1",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.38.0",
    "eslint-config-nicenice": "^4.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-camel-case": "^1.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^21.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "style-loader": "^3.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  }
}
